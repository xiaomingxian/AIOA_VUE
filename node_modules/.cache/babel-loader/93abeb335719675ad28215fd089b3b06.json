g('bardepart----------------------------------',data)\r\n        if(data[0]==null){\r\n          this.banJie =0\r\n        }else{\r\n          this.banJie =data[0].reta*100;\r\n        }\r\n      },\r\n\r\n    },\r\n\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/data/dataGram/chartComponents"}]}