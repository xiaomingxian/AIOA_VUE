utton type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\r\n              <a-button type=\"primary\" @click=\"searchReset\" icon=\"reload\" style=\"margin-left: 8px\">重置</a-button>\r\n              <a @click=\"handleToggleSearch\" style=\"margin-left: 8px\">\r\n                {{ toggleSearchStatus ? '收起' : '展开' }}\r\n                <a-icon :type=\"toggleSearchStatus ? 'up' : 'down'\"/>\r\n              </a>\r\n            </span>\r\n          </a-col>\r\n\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n\r\n    <!-- 操作按钮区域 -->\r\n    <div class=\"table-operator\">\r\n      <a-button @click=\"handleAdd\" type=\"primary\" icon=\"plus\">新增</a-button>\r\n      <a-button type=\"primary\" icon=\"download\" @click=\"handleExportXls('oa发文代码生成测试')\">导出</a-button>\r\n      <a-upload name=\"file\" :showUploadList=\"false\" :multiple=\"false\" :headers=\"tokenHeader\" :action=\"importExcelUrl\" @change=\"handleImportExcel\">\r\n        <a-button type=\"primary\" icon=\"import\">导入</a-button>\r\n      </a-upload>\r\n      <a-dropdown v-if=\"selectedRowKeys.length > 0\">\r\n        <a-menu slot=\"overlay\">\r\n          <a-menu-item key=\"1\" @click=\"batchDel\"><a-icon type=\"delete\"/>删除</a-menu-item>\r\n        </a-menu>\r\n        <a-button style=\"margin-left: 8px\"> 批量操作 <a-icon type=\"down\" /></a-button>\r\n      </a-dropdown>\r\n    </div>\r\n\r\n    <!-- table区域-begin -->\r\n    <div>\r\n      <div class=\"ant-alert ant-alert-info\" style=\"margin-bottom: 16px;\">\r\n        <i class=\"anticon anticon-info-circle ant-alert-icon\"></i> 已选择 <a style=\"font-weight: 600\">{{ selectedRowKeys.length }}</a>项\r\n        <a style=\"margin-left: 24px\" @click=\"onClearSelected\">清空</a>\r\n      </div>\r\n\r\n      <a-table\r\n        ref=\"table\"\r\n        size=\"middle\"\r\n        bordered\r\n        rowKey=\"id\"\r\n        :columns=\"columns\"\r\n        :dataSource=\"dataSource\"\r\n        :pagination=\"ipagination\"\r\n        :loading=\"loading\"\r\n        :rowSelection=\"{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}\"\r\n        @change=\"handleTableChange\">\r\n\r\n        <span slot=\"action\" slot-scope=\"text, record\">\r\n          <a @click=\"handleEdit(record)\">编辑</a>\r\n\r\n          <a-divider type=\"vertical\" />\r\n          <a-dropdown>\r\n            <a class=\"ant-dropdown-link\">更多 <a-icon type=\"down\" /></a>\r\n            <a-menu slot=\"overlay\">\r\n              <a-menu-item>\r\n                <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record.id)\">\r\n                  <a>删除</a>\r\n                </a-popconfirm>\r\n              </a-menu-item>\r\n            </a-menu>\r\n          </a-dropdown>\r\n        </span>\r\n\r\n      </a-table>\r\n    </div>\r\n    <!-- table区域-end -->\r\n\r\n    <!-- 表单区域 -->\r\n    <disFileTest-modal ref=\"modalForm\" @ok=\"modalFormOk\"></disFileTest-modal>\r\n  </a-card>\r\n</template>\r\n\r\n<script>\r\n  import DisFileTestModal from './modules/DisFileTestModal'\r\n  import { JeecgListMixin } from '@/mixins/JeecgListMixin'\r\n\r\n  export default {\r\n    name: \"DisFileTestList\",\r\n    mixins:[JeecgListMixin],\r\n    components: {\r\n      DisFileTestModal\r\n    },\r\n    data () {\r\n      return {\r\n        description: 'oa发文代码生成测试管理页面',\r\n        // 表头\r\n        columns: [\r\n          {\r\n            title: '#',\r\n            dataIndex: '',\r\n            key:'rowIndex',\r\n            width:60,\r\n            align:\"center\",\r\n            customRender:function (t,r,index) {\r\n              return parseInt(index)+1;\r\n            }\r\n           },\r\n\t\t   {\r\n            title: '发文ID',\r\n            align:\"center\",\r\n            dataIndex: 'iid'\r\n           },\r\n\t\t   {\r\n            title: '标题',\r\n            align:\"center\",\r\n            dataIndex: 'stitle'\r\n           },\r\n\t\t   {\r\n            title: '标题传入参数',\r\n            align:\"center\",\r\n            dataIndex: 'stitleparam'\r\n           },\r\n\t\t   {\r\n            title: '标题部门（根据模板中定义的稿纸头级别获取对应的及部门）',\r\n            align:\"center\",\r\n            dataIndex: 'stitledept'\r\n           },\r\n\t\t   {\r\n            title: '密级 1 普通、2 秘密、3 机密、4 决密',\r\n            align:\"center\",\r\n            dataIndex: 'ssafetylevel'\r\n           },\r\n\t\t   {\r\n            title: '拟稿人',\r\n            align:\"center\",\r\n            dataIndex: 'iuserid'\r\n           },\r\n\t\t   {\r\n            title: '拟稿人名称',\r\n            align:\"center\",\r\n            dataIndex: 'susername'\r\n           },\r\n\t\t   {\r\n            title: '拟稿人所在部门',\r\n            align:\"center\",\r\n            dataIndex: 'ideptid'\r\n           },\r\n\t\t   {\r\n            title: '拟稿人所在部门名称',\r\n            align:\"center\",\r\n            dataIndex: 'sdeptname'\r\n           },\r\n\t\t   {\r\n            title: '电话',\r\n            align:\"center\",\r\n            dataIndex: 'sphone'\r\n           },\r\n\t\t   {\r\n            title: '拟稿日期',\r\n            align:\"center\",\r\n            dataIndex: 'tdraftdate'\r\n           },\r\n\t\t   {\r\n            title: '有无附件标示 1 是 0 否',\r\n            align:\"center\",\r\n            dataIndex: 'sfilemark'\r\n           },\r\n\t\t   {\r\n            title: '主题词',\r\n            align:\"center\",\r\n            dataIndex: 'skeyword'\r\n           },\r\n\t\t   {\r\n            title: '发文编号',\r\n            align:\"center\",\r\n            dataIndex: 'snumber'\r\n           },\r\n\t\t   {\r\n            title: '校对',\r\n            align:\"center\",\r\n            dataIndex: 'scollate'\r\n           },\r\n\t\t   {\r\n            title: '打字',\r\n            align:\"center\",\r\n            dataIndex: 'stypewrite'\r\n           },\r\n\t\t   {\r\n            title: '打印份数',\r\n            align:\"center\",\r\n            dataIndex: 'iprintnum'\r\n           },\r\n\t\t   {\r\n            title: '封发',\r\n            align:\"center\",\r\n            dataIndex: 'tsealldate'\r\n           },\r\n\t\t   {\r\n            title: '是否有背景材料 1是 0 否',\r\n            align:\"center\",\r\n            dataIndex: 'sbackgroup'\r\n           },\r\n\t\t   {\r\n            title: '全行发布 1 是 0 否',\r\n            align:\"center\",\r\n            dataIndex: 'sovert'\r\n           },\r\n\t\t   {\r\n            title: '是否会签 1 是 0 否',\r\n            align:\"center\",\r\n            dataIndex: 'scountersign'\r\n           },\r\n\t\t   {\r\n            title: '会签部门IDS(以逗号分隔)',\r\n            align:\"center\",\r\n            dataIndex: 'scrsdeptids'\r\n           },\r\n\t\t   {\r\n            title: '会签部门',\r\n            align:\"center\",\r\n            dataIndex: 'scrsdept'\r\n           },\r\n\t\t   {\r\n            title: '排版标识 1是 0否 默认否 排版后自动更改为 是 ',\r\n            align:\"center\",\r\n            dataIndex: 'ssettype'\r\n           },\r\n\t\t   {\r\n            title: '主送单位',\r\n            align:\"center\",\r\n            dataIndex: 'smainunitids'\r\n           },\r\n\t\t   {\r\n            title: '主送单位名称',\r\n            align:\"center\",\r\n            dataIndex: 'smainunitnames'\r\n           },\r\n\t\t   {\r\n            title: '抄送单位',\r\n            align:\"center\",\r\n            dataIndex: 'sccunitids'\r\n           },\r\n\t\t   {\r\n            title: '抄送单位名称',\r\n            align:\"center\",\r\n            dataIndex: 'sccunitnames'\r\n           },\r\n\t\t   {\r\n            title: '内部发送',\r\n            align:\"center\",\r\n            dataIndex: 'sinsideids'\r\n           },\r\n\t\t   {\r\n            title: '内部部门名称',\r\n            align:\"center\",\r\n            dataIndex: 'sinsidedeptname'\r\n           },\r\n\t\t   {\r\n            title: 'sreportids',\r\n            align:\"center\",\r\n            dataIndex: 'sreportids'\r\n           },\r\n\t\t   {\r\n            title: 'sreportnames',\r\n            align:\"center\",\r\n            dataIndex: 'sreportnames'\r\n           },\r\n\t\t   {\r\n            title: '缓急 1 一般 2 急件 3 特急',\r\n            align:\"center\",\r\n            dataIndex: 'surgency'\r\n           },\r\n\t\t   {\r\n            title: '政务公开 1 是 0 否',\r\n            align:\"center\",\r\n            dataIndex: 'saffairsovert'\r\n           },\r\n\t\t   {\r\n            title: '不公开理由',\r\n            align:\"center\",\r\n            dataIndex: 'saccount'\r\n           },\r\n\t\t   {\r\n            title: '签发时间',\r\n            align:\"center\",\r\n            dataIndex: 'tissuedate'\r\n           },\r\n\t\t   {\r\n            title: '修改时间',\r\n            align:\"center\",\r\n            dataIndex: 'tupdate'\r\n           },\r\n\t\t   {\r\n            title: '稿纸头名称（取值于模板 <稿纸头名称>）',\r\n            align:\"center\",\r\n            dataIndex: 'spapername'\r\n           },\r\n\t\t   {\r\n            title: '文件类型 1 发文、2 签报、',\r\n            align:\"center\",\r\n            dataIndex: 'sfiletype'\r\n           },\r\n\t\t   {\r\n            title: '状态 1 办结 0 未办结',\r\n            align:\"center\",\r\n            dataIndex: 'sstate'\r\n           },\r\n\t\t   {\r\n            title: '备注',\r\n            align:\"center\",\r\n            dataIndex: 'sremarks'\r\n           },\r\n\t\t   {\r\n            title: 'itimelimit',\r\n            align:\"center\",\r\n            dataIndex: 'itimelimit'\r\n           },\r\n\t\t   {\r\n            title: 'ipaginalnum',\r\n            align:\"center\",\r\n            dataIndex: 'ipaginalnum'\r\n           },\r\n\t\t   {\r\n            title: 'sreason',\r\n            align:\"center\",\r\n            dataIndex: 'sreason'\r\n           },\r\n\t\t   {\r\n            title: '冗余',\r\n            align:\"center\",\r\n            dataIndex: 'sredundance'\r\n           },\r\n\t\t   {\r\n            title: '冗余1',\r\n            align:\"center\",\r\n            dataIndex: 'sredundance1'\r\n           },\r\n\t\t   {\r\n            title: 'processinstid',\r\n            align:\"center\",\r\n            dataIndex: 'processinstid'\r\n           },\r\n\t\t   {\r\n            title: '所选文号ID',\r\n            align:\"center\",\r\n            dataIndex: 'idocid'\r\n           },\r\n\t\t   {\r\n            title: 'itemplateid',\r\n            align:\"center\",\r\n            dataIndex: 'itemplateid'\r\n           },\r\n\t\t   {\r\n            title: '签发人',\r\n            align:\"center\",\r\n            dataIndex: 'sissueuser'\r\n           },\r\n\t\t   {\r\n            title: '签发日期 格式：2016-03-11',\r\n            align:\"center\",\r\n            dataIndex: 'dissuedate'\r\n           },\r\n\t\t   {\r\n            title: '中文签发日期（格式：二○一六年三月二十一号）',\r\n            align:\"center\",\r\n            dataIndex: 'dzhcnissuedate'\r\n           },\r\n\t\t   {\r\n            title: '规范性标志 1 是 0 否',\r\n            align:\"center\",\r\n            dataIndex: 'sstandardflag'\r\n           },\r\n\t\t   {\r\n            title: '发送地市中支Ids',\r\n            align:\"center\",\r\n            dataIndex: 'sprovissuedcityids'\r\n           },\r\n          {\r\n            title: '操作',\r\n            dataIndex: 'action',\r\n            align:\"center\",\r\n            scopedSlots: { customRender: 'action' },\r\n          }\r\n        ],\r\n\t\turl: {\r\n          list: \"/oaDisFileTest/disFileTest/list\",\r\n          delete: \"/oaDisFileTest/disFileTest/delete\",\r\n          deleteBatch: \"/oaDisFileTest/disFileTest/deleteBatch\",\r\n          exportXlsUrl: \"oaDisFileTest/disFileTest/exportXls\",\r\n          importExcelUrl: \"oaDisFileTest/disFileTest/importExcel\",\r\n       },\r\n    }\r\n  },\r\n  computed: {\r\n    importExcelUrl: function(){\r\n      return `${window._CONFIG['domianURL']}/${this.url.importExcelUrl}`;\r\n    }\r\n  },\r\n    methods: {\r\n     \r\n    }\r\n  }\r\n</script>\r\n<style scoped>\r\n  @import '~@assets/less/common.less'\r\n</style>"],"sourceRoot":"src/views/jeecg"}]}