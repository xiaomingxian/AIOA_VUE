AgentModal\";\nimport SysUserManagedeptsList from \"./SysUserManagedeptsList\";\nexport default {\n  name: \"UserList\",\n  mixins: [JeecgListMixin],\n  components: {\n    SysUserAgentModal: SysUserAgentModal,\n    UserModal: UserModal,\n    PasswordModal: PasswordModal,\n    SysUserManagedeptsList: SysUserManagedeptsList\n  },\n  data: function data() {\n    var _this = this;\n\n    return {\n      description: '这是用户管理页面',\n      queryParam: {},\n      columns: [\n      /*{\n        title: '#',\n        dataIndex: '',\n        key:'rowIndex',\n        width:60,\n        align:\"center\",\n        customRender:function (t,r,index) {\n          return parseInt(index)+1;\n        }\n      },*/\n      {\n        title: '部门',\n        align: \"center\",\n        dataIndex: 'departName',\n        width: 200,\n        sorter: function sorter(i, ii, type) {\n          _this.queryParam.orederBydepart = type == 'descend' ? -1 : 1;\n          return true;\n        },\n        customRender: function customRender(text, record, index) {\n          // console.log(record)\n          if (record.parentName) {\n            return record.parentName + '-' + text;\n          } else {\n            return text;\n          }\n        }\n      }, {\n        title: '用户账号',\n        align: \"center\",\n        dataIndex: 'username',\n        width: 200\n      }, {\n        title: '真实姓名',\n        align: \"center\",\n        width: 200,\n        dataIndex: 'realname'\n      },\n      /*{\n        title: '性别',\n        align: \"center\",\n        width: 80,\n        dataIndex: 'sex_dictText',\n        sorter: true\n      },*/\n\n      /*{\n        title: '头像',\n        align: \"center\",\n        width: 120,\n        dataIndex: 'avatar',\n        scopedSlots: {customRender: \"avatarslot\"}\n      },\n        {\n        title: '生日',\n        align: \"center\",\n        width: 180,\n        dataIndex: 'birthday'\n      },\n      {\n        title: '手机号码',\n        align: \"center\",\n        width: 100,\n        dataIndex: 'phone'\n      },*/\n      {\n        title: '邮箱',\n        align: \"center\",\n        width: 200,\n        dataIndex: 'email'\n      }, {\n        title: '状态',\n        align: \"center\",\n        width: 200,\n        dataIndex: 'status_dictText'\n      },\n      /* {\n         title: '创建时间',\n         align: \"center\",\n         width: 150,\n         dataIndex: 'createTime',\n         sorter: true\n       },*/\n      {\n        title: '操作',\n        dataIndex: 'action',\n        scopedSlots: {\n          customRender: 'action'\n        },\n        align: \"center\",\n        width: 200\n      }],\n      url: {\n        imgerver: window._CONFIG['domianURL'] + \"/sys/common/view\",\n        syncUser: \"/process/extActProcess/doSyncUser\",\n        list: \"/sys/user/list\",\n        delete: \"/sys/user/delete\",\n        deleteBatch: \"/sys/user/deleteBatch\",\n        exportXlsUrl: \"/sys/user/exportXls\",\n        importExcelUrl: \"sys/user/importExcel\"\n      }\n    };\n  },\n  computed: {\n    importExcelUrl: function importExcelUrl() {\n      return \"\".concat(window._CONFIG['domianURL'], \"/\").concat(this.url.importExcelUrl);\n    }\n  },\n  methods: {\n    getAvatarView: function getAvatarView(avatar) {\n      return this.url.imgerver + \"/\" + avatar;\n    },\n    batchFrozen: function batchFrozen(status) {\n      if (this.selectedRowKeys.length <= 0) {\n        this.$message.warning('请选择一条记录！');\n        return false;\n      } else {\n        var ids = \"\";\n        var that = this;\n        var isAdmin = false;\n        that.selectionRows.forEach(function (row) {\n          if (row.username == 'admin') {\n            isAdmin = true;\n          }\n        });\n\n        if (isAdmin) {\n          that.$message.warning('管理员账号不允许此操作,请重新选择！');\n          return;\n        }\n\n        that.selectedRowKeys.forEach(function (val) {\n          ids += val + \",\";\n        });\n        that.$confirm({\n          title: \"确认操作\",\n          content: \"是否\" + (status == 1 ? \"解冻\" : \"冻结\") + \"选中账号?\",\n          onOk: function onOk() {\n            frozenBatch({\n              ids: ids,\n              status: status\n            }).then(function (res) {\n              if (res.success) {\n                that.$message.success(res.message);\n                console.log(\"-----------------------=======================\");\n                that.loadData();\n                that.onClearSelected();\n              } else {\n                that.$message.warning(res.message);\n              }\n            });\n          }\n        });\n      }\n    },\n    handleMenuClick: function handleMenuClick(e) {\n      if (e.key == 1) {\n        this.batchDel();\n      } else if (e.key == 2) {\n        this.batchFrozen(2);\n      } else if (e.key == 3) {\n        this.batchFrozen(1);\n      }\n    },\n    handleFrozen: function handleFrozen(id, status, username) {\n      var that = this; //TODO 后台校验管理员角色\n\n      if ('admin' == username) {\n        that.$message.warning('管理员账号不允许此操作！');\n        return;\n      }\n\n      frozenBatch({\n        ids: id,\n        status: status\n      }).then(function (res) {\n        if (res.success) {\n          that.$message.success(res.message);\n          console.log(\"----------------222222-------=======================\");\n          that.loadData();\n        } else {\n          that.$message.warning(res.message);\n        }\n      });\n    },\n    handleChangePassword: function handleChangePassword(username) {\n      this.$refs.passwordmodal.show(username);\n    },\n    handleAgentSettings: function handleAgentSettings(username) {\n      this.$refs.sysUserAgentModal.agentSettings(username);\n      this.$refs.sysUserAgentModal.title = \"用户代理人设置\";\n    },\n    handleAgentConfig: function handleAgentConfig(username) {\n      this.$refs.SysUserManagedeptsList.config(username);\n      this.$refs.SysUserManagedeptsList.title = \"分管部门管理\";\n    },\n    passwordModalOk: function p