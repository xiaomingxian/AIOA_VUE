   // 根据屏幕变化,设置抽屉尺寸\r\n      resetScreenSize() {\r\n        let screenWidth = document.body.clientWidth\r\n        if (screenWidth < 500) {\r\n          this.drawerWidth = screenWidth\r\n        } else {\r\n          this.drawerWidth = 650\r\n        }\r\n      },\r\n      getRowClassname(record){\r\n        if(record.status!=1){\r\n          return \"data-rule-invalid\"\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style>\r\n  .data-rule-invalid{\r\n    background: #f4f4f4;\r\n    color: #bababa;\r\n  }\r\n</style>"]}]}